{
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    // below adds support of 'define' and 'require'
    "amd": true
  },
  "extends": [
    "@oracle/eslint-config-oraclejet"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off"
  }
}
